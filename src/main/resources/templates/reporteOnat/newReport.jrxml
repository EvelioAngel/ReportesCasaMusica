	<queryString>
		<![CDATA[SELECT  
  SUM(m1.precio) as Contrato,
  m1.porciento_empresa,  
  SUM(nomina_artista.salario_bruto) as Salario 
FROM   
  nomina_artista,  
  m1,
  nomina,
  m2
WHERE 
  nomina_artista.id_nomina = nomina.id_nomina AND  
  nomina.anno = 2017 AND 
  nomina_artista.idm2 = m2.id_m2 AND
  m2.id_m1 = m1.id_m1 AND
  nomina_artista.idartista = 1202 
GROUP BY
  m1.porciento_empresa
ORDER BY
  porciento_empresa DESC]]>
	</queryString>
	<field name="contrato" class="java.math.BigDecimal"/>
	<field name="porciento_empresa" class="java.math.BigDecimal"/>
	<field name="salario" class="java.math.BigDecimal"/>
